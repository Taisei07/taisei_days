<p id="notice"><%= notice %></p>

<h1>Nappis</h1>

<table>
  <thead>
    <tr>
      <th>Year</th>
      <th>Age</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nappis.each do |nappi| %>
      <tr>
        <td><%= nappi.year %></td>
        <td><%= nappi.age %></td>
        <td><%= link_to 'Show', nappi %></td>
        <td><%= link_to 'Edit', edit_nappi_path(nappi) %></td>
        <td><%= link_to 'Destroy', nappi, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Nappi', new_nappi_path %>
